    <!-- https://w3id.org/ride2rail/terms#Consumable -->


    <owl:Class rdf:about="https://w3id.org/ride2rail/terms#Consumable">
        <rdfs:comment>A type of entity that can be listed in an InventoryAllocation and can be consumed performing a InventoryAllocation.</rdfs:comment>
        <rdfs:label>Consumable</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ride2rail/terms#DiscountVoucher -->


    <owl:Class rdf:about="https://w3id.org/ride2rail/terms#DiscountVoucher">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ride2rail/terms#Voucher"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ride2rail/terms#discountPercentage"/>
